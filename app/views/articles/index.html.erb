<%=render 'articles/header'%>
<% @articles.each do |article|%>
  <%unless article.archived?%>
    <div class="card" style="width: 18rem;">
      <div class="card-body">
        <h5 class="card-title"><%= link_to article.title, article %></h5>
        <p class="card-text"><%=article.body%></p>
      </div>
    </div>
  <%end%>
<%end%>
